  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/lectures/chap2/c2v2",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "debug: test1",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/homework/cp2a/test1",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/homework/cp2a",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb"
      // "preLaunchTask": "make file"
    }
  ]
}
